
<div class="container pt-4 col-4">
  <h1>Create List</h1>
  <%= simple_form_for @list do |f| %>
    <%= f.input :name %> <br>
    <%= f.submit class: 'btn btn-success text-white' %>
  <% end %>

  <br>

  <%= link_to "Back to lists", lists_path, class: 'btn btn-danger text-white', style: 'margin: 10px;' %>
</div>
